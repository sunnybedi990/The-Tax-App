<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.dalbeer.sunnybedi.taxes">

    <application
        android:allowBackup="true"
        android:icon="@drawable/taxappicon"
        android:label="@string/app_name"
        android:roundIcon="@drawable/taxappicon"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".ScrollingActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".tds" />
        <activity
            android:name=".tds_category"
            android:label="@string/title_activity_tds_category"
            android:theme="@style/AppTheme" />
        <activity android:name=".tds_calculator" />
        <activity
            android:name=".tds_interest_calculator"
            android:label="@string/title_activity_tds_interest_calculator"
            android:theme="@style/AppTheme" />
    </application>

</manifest>